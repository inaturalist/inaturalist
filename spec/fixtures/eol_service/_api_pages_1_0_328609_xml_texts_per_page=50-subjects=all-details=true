      <dataObject>
        <identifier>EOL-article-801-4562f27f0bfc735d0dbdc389052e9d43</identifier>
        <dataObjectVersionID type="integer">18001887</dataObjectVersionID>
        <dataType>http://purl.org/dc/dcmitype/Text</dataType>
