        <identifier>EOL-article-766-VUVU_COVER_REQUIREMENTS</identifier>
        <dataObjectVersionID type="integer">13940000</dataObjectVersionID>
        <dataType>http://purl.org/dc/dcmitype/Text</dataType>
        <dataSubtype></dataSubtype>
