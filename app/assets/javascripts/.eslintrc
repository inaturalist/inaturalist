{
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "requireConfigFile": false,
  },
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": false,
    "jquery": true,
    "es6": false
  },
  "ignorePatterns": [
    "*-webpack.js"
  ],
  "rules": {
    "arrow-parens": [2, "as-needed"],
    "comma-dangle": [2, "never"],
    "no-var": 0,
    "func-names": 0,
    "prefer-arrow-callback": 0,
    "prefer-template": 0,
    "vars-on-top": 0,
    "object-shorthand": 0,
    "prefer-destructuring": [2, { "object": true, "array": false }],
    "quotes": [2, "double"],
    "space-in-parens": [2, "always"],
  }
}
