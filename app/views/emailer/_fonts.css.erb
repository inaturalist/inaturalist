@font-face {
  font-family: Lato;
  src: url("<%= asset_url 'fonts/Lato/Lato-Regular.eot', host: Site.default.url, skip_pipeline: true %>"); /* IE9 Compat Modes */
  src: url("<%= asset_url 'fonts/Lato/Lato-Regular.eot?#iefix', host: Site.default.url, skip_pipeline: true %>") format("embedded-opentype"), /* IE6-IE8 */
       url("<%= asset_url 'fonts/Lato/Lato-Regular.woff2', host: Site.default.url, skip_pipeline: true %>") format("woff2"), /* Modern Browsers */
       url("<%= asset_url 'fonts/Lato/Lato-Regular.woff', host: Site.default.url, skip_pipeline: true %>") format("woff"), /* Modern Browsers */
       url("<%= asset_url 'fonts/Lato/Lato-Regular.ttf', host: Site.default.url, skip_pipeline: true %>") format("truetype");
  font-style: normal;
  font-weight: 400;
  text-rendering: optimizeLegibility;
}

