<%- paginator ||= page_results -%>
<span class="description">
  <% if paginator.total_entries > 0 -%>
    <%= paginator.offset == 0 ? 1 : paginator.offset %> &ndash; 
    <%= [paginator.offset + paginator.per_page, paginator.total_entries].min %> <%= t :of %>
  <% end -%>
  <span class="count"><%= number_with_delimiter paginator.total_entries %></span>
  <%= t(:results).downcase if paginator.total_pages == 1 %>
</span>
