- terms.each do | term |
  %h3
    %a{ name: term, href: "##{term}" }
      %code= term
  %table.table.term-table
    %tr
      %th=t :definition
      %td=define_term.call term
    %tr
      %th URI
      %td= link_to term_uri_url( term: term, host: Site.default.url ), term_uri_url( term: term, host: Site.default.url )
